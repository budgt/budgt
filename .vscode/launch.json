{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "ng serve categorylist",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200/",
      "runtimeExecutable": "/usr/bin/google-chrome-stable",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "breakOnLoad": true,
      "sourceMapPathOverrides": {
        "*": "${webRoot}/*"
      }
    }
  ]
}
